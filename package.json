{
	"name": "learnpack-vscode",
	"displayName": "learnpack-vscode",
	"description": "Learn and upgrade your coding skills one package at a time",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:learnpack-vscode.startTerminal",
		"onCommand:learnpack-vscode.openCurrentExercise",
		"onCommand:learnpack-vscode.showInstructions"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "learnpack-vscode.showInstructions",
				"title": "Learnpack: Start"
			},
			{
				"command": "learnpack-vscode.openCurrentExercise",
				"title": "Learnpack: Open Current Exercise"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"chokidar": "^3.5.2",
		"shelljs": "^0.8.4",
		"socket.io": "^4.1.2",
		"socket.io-client": "^4.1.2"
	}
}
